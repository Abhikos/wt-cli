#!/usr/bin/env node
var Cli = require('../cli');
var Package = require('../package.json');


var cli = Cli.app({
    description: Package.description,
    version: Package.version,
});


cli.addCommand(require('./init'));
cli.addCommand(require('./create'));
cli.addCategory(require('./profile'));
cli.addCommand(require('./logs'));


Cli.run(cli, { timeout: 1000 * 20 });